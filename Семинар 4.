 // Напишите цикл, который принимает на вход два числа (A и B) и возводит число A в натуральную степень B.
 
  // 3, 5 -> 243 (3⁵)
 
  
  static void  Main(string[] args) 
  
  {
		double 3
		double 5
		double result = Math.Pow(3, 5);
		Console.WritLine(result);
  }
 
	 // 2, 4 -> 16
	 
